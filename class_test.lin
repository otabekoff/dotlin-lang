class Person {
    val name: String
    val age: Int
    
    constructor(name: String, age: Int) {
        this.name = name
        this.age = age
    }
    
    fun greet(): String {
        return "Hello, I'm " + this.name + " and I'm " + this.age + " years old"
    }
}

fun main() {
    val person = Person("Alice", 30)
    println(person.greet())
}
