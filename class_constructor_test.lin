class Person {
    var name: String
    var age: Int
    
    constructor(name: String, age: Int) {
        this.name = name
        this.age = age
    }
    
    fun getName(): String {
        return this.name
    }
    
    fun getAge(): Int {
        return this.age
    }
}

fun main() {
    val person = Person("John", 30)
    println(person.getName())
    println(person.getAge().toString())
}