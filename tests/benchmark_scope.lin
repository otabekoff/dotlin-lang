fun benchmark() {
    var a = "global";
    {
        var b = "first";
        {
            var c = "second";
            {
                var d = "third";
                {
                    var e = "fourth";
                    {
                        var f = "fifth";
                        // Access variables from different depths 1,000,000 times
                        var start = currentTimeMicros();
                        var i = 0;
                        var x = a;
                        var y = b;
                        var z = e;
                        while(i < 2000000) {
                            x = a; 
                            y = b; 
                            z = e; 
                            i = i + 1;
                        }
                        var end = currentTimeMicros();
                        println("Time taken: " + (end - start) + "us");
                        println("Final i: " + i);
                    }
                }
            }
        }
    }
}

benchmark();
