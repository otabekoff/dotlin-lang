class Person {
    val name: String
    val age: Int
    
    constructor(name: String, age: Int) {
        this.name = name
        this.age = age
    }
    
    fun greet() {
        println("Hello, my name is " + this.name + " and I am " + this.age + " years old")
    }
}

fun main() {
    val person = Person("Alice", 30)
    println("Created person")
    person.greet()
    println("Name: " + person.name)
    println("Age: " + person.age)
}
