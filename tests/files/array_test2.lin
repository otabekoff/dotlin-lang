fun main() {
    // Test array construction
    val arr = arrayOf(1, 2, 3, 4, 5)
    println("Array size: " + arr.size)
    
    // Test array indexing
    println("Element at index 2: " + arr[2])
    
    // Test array methods
    println("First element: " + arr.get(0))
    arr.set(1, 99)
    println("After set(1, 99): " + arr[1])
}
